{
  "compilerOptions": {
    "declaration": false,
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "allowJs": false,
    "removeComments": false,
    "strict": true,
    "target": "esnext",
    "rootDir": "./src",
    "outDir": "./dist",
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}
